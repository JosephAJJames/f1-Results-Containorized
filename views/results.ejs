<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/a.css">
    <title>Formula One Most Recent Results</title>
</head>
<body>
    <div class = "cont">
      <div class="track_and_title">
        <img src="<%= helpers.get_circuit_map_url(round_num) %>" alt="Track map">
        <h4><%= gp_title %></h4>
      </div>
        
      <div class="table_cont">
          <table>
            <tr>
              <th>Name</th>
              <th>face</th>
              <th>Number</th>
              <th>Position</th>
            </tr>
            <% results.forEach((item) => { %>
              <% const num = item.number %>
              <tr>
                <td> <%= item.Driver.givenName + " " + item.Driver.familyName %> </td>
                <td>
                  <div class="face_img_box"><img src="<%= helpers.get_driver_face_url(num) %>" alt="face"></div>
                </td>
                <td> <%= num %> </td>
                <td> <%= item.position %> </td>
              </tr>
            <% }) %>
          </table>
      </div>
    </div>
</body>
</html>